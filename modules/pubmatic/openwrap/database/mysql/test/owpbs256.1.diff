+	// }
+
+	if rCtx.Debug {
+		extBidResponse.OwLogger, _ = pubmatic.GetLogAuctionObjectAsURL(ao, false, true)
+	}
+
+	bidResponse.Ext, _ = json.Marshal(extBidResponse)
