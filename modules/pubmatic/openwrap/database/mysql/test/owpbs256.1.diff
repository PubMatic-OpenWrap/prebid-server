+			name: "Test singleton again",
+			args: args{
+				interval:   "2m",
+				timeout:    "2s",
+				httpClient: http.DefaultClient,
+			},
+			want:    GetExpectedVendorListScheduler("2m", "2s", http.DefaultClient),
+			wantErr: false,
+		},
+	}
