+						},
+						{
+							Bid:               &openrtb2.Bid{ID: "1-ed72b572-ba62-4220-abba-c19c0bf6346b", Price: 3.329644588311827},
+							DealTierSatisfied: true,
+						},
+						{
+							Bid:               &openrtb2.Bid{ID: "5-VIDEO12-89A1-41F1-8708-978FD3C0912A", Price: 5},
