+			case "title":
+				newContentObject.Title = ""
+			case "series":
+				newContentObject.Series = ""
+			case "season":
+				newContentObject.Season = ""
+			case "artist":
+				newContentObject.Artist = ""
+			case "genre":
+				newContentObject.Genre = ""
+			case "album":
+				newContentObject.Album = ""
+			case "isrc":
