+import (
+	"database/sql"
+	"sync"
+
+	"github.com/prebid/prebid-server/modules/pubmatic/openwrap/config"
+)
+
+type mySqlDB struct {
+	conn *sql.DB
+	cfg  config.Database
+}
+
+var db *mySqlDB
+var dbOnce sync.Once
+
+func New(conn *sql.DB, cfg config.Database) *mySqlDB {
