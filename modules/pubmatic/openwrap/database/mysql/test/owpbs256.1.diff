+func createNewContentObject(contentObject *openrtb2.Content, include bool, keys []string) *openrtb2.Content {
+	newContentObject := &openrtb2.Content{}
+	if !include {
+		*newContentObject = *contentObject
+		for _, key := range keys {
+
+			switch key {
+			case "id":
