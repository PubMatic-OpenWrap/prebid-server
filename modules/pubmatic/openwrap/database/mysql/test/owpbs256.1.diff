+		if impCtx.VideoAdUnitCtx.AppliedSlotAdUnitConfig != nil {
+			adUnitCfg = impCtx.VideoAdUnitCtx.AppliedSlotAdUnitConfig
+			break
+		}
+	}
+
+	if adUnitCfg == nil || adUnitCfg.Floors == nil {
+		return
+	}
+
+	requestExt.Prebid.Floors = adUnitCfg.Floors
+}
