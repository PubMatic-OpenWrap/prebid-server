+import "strings"
+
+// VASTTag contains tag details of VASTBidders
+type VASTTag struct {
+	ID        int     `json:"id,omitempty"`
+	PartnerID int     `json:"partnerId,omitempty"`
+	URL       string  `json:"url,omitempty"`
+	Duration  int     `json:"dur,omitempty"`
