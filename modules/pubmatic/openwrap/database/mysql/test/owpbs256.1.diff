+type RequestCtx struct {
+	PubID, ProfileID, DisplayID, VersionID int
+	SSAuction                              int
+	SummaryDisable                         int
+	LogInfoFlag                            int
+	SSAI                                   string
+	PartnerConfigMap                       map[int]map[string]string
