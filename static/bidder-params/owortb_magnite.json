{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "magnite (oRTB Integration) Adapter Params",
    "description": "A schema which validates params accepted by the magnite (oRTB Integration) adapter",
    "type": "object",
    "properties": {
      "adunitID": {
        "type": "string",
        "description": "adunitID param",
        "location": "app.MyAdunitID"
      },
      "mykey": {
        "type": "string",
        "description": "mykey param",
        "location": "site.myKey"
      },
      "BidResponse": {
        "BidType": {
            "location": "resp.seatbid.bid.ext.bidtype"
         }
      }
    },
    "required": ["adunitID"],
    "responseMapper": {
      "bidtype":
        {
          "level": "bid",
          "location": "ext.mtype"
        }
    }
  }
